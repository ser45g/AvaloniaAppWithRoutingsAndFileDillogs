<UserControl
    x:Class="TestRoutingProject.Views.NoSettingsAvailableView"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="using:TestRoutingProject.ViewModels"
   
    x:DataType="vm:NoSettingsAvailableViewModel"
    mc:Ignorable="d">
    <Grid>
        <StackPanel
            HorizontalAlignment="Center"
            VerticalAlignment="Center"
            Spacing="10">
            <TextBlock Text="Нет доступных настроек для данного шифра" />
            <Button HorizontalAlignment="Center" Command="{Binding CloseCommand}">
               Ok
            </Button>
        </StackPanel>

    </Grid>
</UserControl>
